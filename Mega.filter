Show	#Gear - Progression - Weapon - Sceptres - Lowlevel
	Class "Sceptres"
	Rarity <= Magic
	ItemLevel <= 10
	SetFontSize 28
Hide	#Gear - Progression - Weapon - Sceptres - No Sockets
	Class "Sceptres"
	ItemLevel < 77
	Rarity <= Magic
	Sockets < 3
	SetFontSize 28
Show	#Gear - Progression - Weapon - Sceptres - 3/4 Linked Sockets
	Class "Sceptres"
	ItemLevel < 77
	Rarity <= Magic
	LinkedSockets >= 3
	SetFontSize 28
Hide	#Gear - Progression - Weapon - Sceptres - 3/4 Sockets
	Class "Sceptres"
	ItemLevel < 77
	Rarity <= Magic
	Sockets >= 3
	SetFontSize 28
